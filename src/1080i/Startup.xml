<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">10</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<onload>Skin.SetBool(showRecentVideoItems)</onload>
	<onload>Skin.SetBool(showRecentMusicItems)</onload>
	<onload condition="!Skin.HasSetting(DebugInfo)">Skin.SetString(DebugInfo,false)</onload>
	<onload condition="!Skin.HasSetting(DebugGrid)">Skin.SetString(DebugGrid,false)</onload>
	<onload condition="!Skin.HasSetting(HomeArtistViewer)">Skin.SetString(HomeArtistViewer,true)</onload>
	<onload condition="!Skin.HasSetting(VisualizationArtistViewer)">Skin.SetString(VisualizationArtistViewer,true)</onload>
	<coordinates>
		<posx>15</posx>
		<posy>15</posy>
	</coordinates>

	<controls>
		<control type="button" id="10">
			<description>Startup Trigger</description>
			<onfocus>ReplaceWindow(home)</onfocus>
			<texturenofocus>-</texturenofocus>
			<texturefocus>-</texturefocus>
		</control>

		<!--debugging-->
		<include>DebugGrid</include>
		<control type="label">
			<include>DebugWindowID</include>
			<label>StartUp.xml</label>
		</control>
	</controls>
</window>
